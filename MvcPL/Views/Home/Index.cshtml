@model  MvcPL.ViewModels.IndexViewModel<MvcPL.ViewModels.PhotoViewModel>

@{
    ViewBag.Title = "Home Page";
}
<head>
    <link href='@Url.Content("~/Content/themes/base/all.css")' rel="stylesheet" type="text/css" />
</head>
<div id="photos-list">
    @Html.Partial("ShowPhotos", Model);
</div>

@section Scripts {

@Scripts.Render("~/scripts/jquery-1.10.2.js")
@Scripts.Render("~/scripts/jquery.unobtrusive-ajax.js")
@Scripts.Render("~/Scripts/jquery-ui-1.12.0.min.js")
<script type="text/javascript">
    $(function () {
        var autocompleteUrl = '@Url.Action("TagNameFinder")';
        $("input#tagName").autocomplete({
            source: autocompleteUrl,
            minLength: 2,
            select: function (event, ui) {
                alert("Selected " + ui.item.label);
            }
        });
    });
</script>
}
